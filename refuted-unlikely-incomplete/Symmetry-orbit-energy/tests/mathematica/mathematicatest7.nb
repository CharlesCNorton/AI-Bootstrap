         RowBox[{"group", ",", "groups"}], "}"}]}], "]"}], "]"}]}], ";"}], 
   "\[IndentingNewLine]", "\n", 
   RowBox[{"(*", 
    RowBox[{
    "Display", " ", "Results", " ", "and", " ", "Analyze", " ", "Consistency",
      " ", "with", " ", "Hypotheses"}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Print", "[", 
     RowBox[{
     "\"\<All calculations completed. Results: \>\"", ",", "results"}], "]"}],
     ";"}], "\n", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
    "Print", "[", 
     "\"\<Analyzing SOE Consistency with Rank and Symmetry Structure...\>\"", 
     "]"}], ";"}], "\n", 
   RowBox[{
    RowBox[{"rankOrder", "=", 
     RowBox[{"Association", "[", 
      RowBox[{"MapIndexed", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"#1", "->", 
          RowBox[{"First", "[", "#2", "]"}]}], "&"}], ",", "groups"}], "]"}], 
      "]"}]}], ";"}], "\[IndentingNewLine]", "\n", 
   RowBox[{"(*", 
    RowBox[{"Check", " ", "SOE", " ", "Order", " ", "with", " ", "Rank"}], 
