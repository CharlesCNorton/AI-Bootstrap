    "Define", " ", "General", " ", "Haar", " ", "Measure", " ", "and", " ", 
     "Density", " ", "Functions"}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"haarMeasure", "[", "group_", "]"}], ":=", 
     RowBox[{"Switch", "[", 
      RowBox[{"group", ",", "\"\<SU(n)\>\"", ",", 
       RowBox[{"1", "/", 
        RowBox[{"(", 
         RowBox[{"2", " ", 
          RowBox[{"Pi", "^", 
           RowBox[{"(", 
            RowBox[{"n", "-", "1"}], ")"}]}]}], ")"}]}], ",", "\"\<SO(n)\>\"",
        ",", 
       RowBox[{"1", "/", 
        RowBox[{"(", 
         RowBox[{"4", " ", 
          RowBox[{"Pi", "^", 
           RowBox[{"(", 
            RowBox[{"n", "-", "1"}], ")"}]}]}], ")"}]}], ",", "\"\<Sp(n)\>\"",
        ",", 
       RowBox[{"1", "/", 
        RowBox[{"(", 
         RowBox[{"8", " ", 
          RowBox[{"Pi", "^", 
           RowBox[{"(", "n", ")"}]}]}], ")"}]}], ",", "\"\<G2\>\"", ",", 
       RowBox[{"1", "/", 
        RowBox[{"(", 
         RowBox[{"16", " ", 
          RowBox[{"Pi", "^", "3"}]}], ")"}]}], ",", "\"\<F4\>\"", ",", 
       RowBox[{"1", "/", 
        RowBox[{"(", 
         RowBox[{"32", " ", 
          RowBox[{"Pi", "^", "4"}]}], ")"}]}], ",", "\"\<E6\>\"", ",", 
       RowBox[{"1", "/", 
        RowBox[{"(", 
         RowBox[{"72", " ", 
          RowBox[{"Pi", "^", "5"}]}], ")"}]}], ",", "\"\<E7\>\"", ",", 
       RowBox[{"1", "/", 
        RowBox[{"(", 
         RowBox[{"144", " ", 
          RowBox[{"Pi", "^", "6"}]}], ")"}]}], ",", "\"\<E8\>\"", ",", 
       RowBox[{"1", "/", 
        RowBox[{"(", 
         RowBox[{"240", " ", 
          RowBox[{"Pi", "^", "7"}]}], ")"}]}], ",", "_", ",", 
       RowBox[{"1", "/", 
        RowBox[{"(", 
         RowBox[{"2", " ", 
          RowBox[{"Pi", "^", "2"}]}], ")"}]}]}], "]"}]}], ";"}], "\n", 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"densityFunction", "[", 
      RowBox[{"group_", ",", "theta_", ",", "phi_"}], "]"}], ":=", 
     RowBox[{"Switch", "[", 
      RowBox[{"group", ",", "\"\<SU(2)\>\"", ",", 
       RowBox[{
        RowBox[{"Sin", "[", "theta", "]"}], "+", 
        RowBox[{"10", "^", 
         RowBox[{"-", "4"}]}]}], ",", "\"\<SU(3)\>\"", ",", 
       RowBox[{
        RowBox[{
         RowBox[{"Sin", "[", "theta", "]"}], "*", 
         RowBox[{
          RowBox[{"Cos", "[", "phi", "]"}], "^", "2"}]}], "+", 
        RowBox[{"10", "^", 
         RowBox[{"-", "4"}]}]}], ",", "\"\<SO(3)\>\"", ",", 
       RowBox[{
        RowBox[{"Sin", "[", "theta", "]"}], "+", 
        RowBox[{"10", "^", 
         RowBox[{"-", "4"}]}]}], ",", "\"\<SO(4)\>\"", ",", 
       RowBox[{
        RowBox[{
         RowBox[{"Sin", "[", "theta", "]"}], "^", "2"}], "+", 
        RowBox[{"10", "^", 
         RowBox[{"-", "4"}]}]}], ",", "\"\<Sp(2)\>\"", ",", 
       RowBox[{
        RowBox[{
         RowBox[{"Sin", "[", "theta", "]"}], "^", "2"}], "+", 
        RowBox[{"10", "^", 
         RowBox[{"-", "4"}]}]}], ",", "\"\<G2\>\"", ",", 
       RowBox[{
        RowBox[{
         RowBox[{"Sin", "[", "theta", "]"}], "*", 
         RowBox[{
          RowBox[{"Cos", "[", "phi", "]"}], "^", "2"}]}], "+", 
        RowBox[{"10", "^", 
         RowBox[{"-", "4"}]}]}], ",", "\"\<F4\>\"", ",", 
       RowBox[{
        RowBox[{
         RowBox[{"Sin", "[", "theta", "]"}], "^", "3"}], "+", 
        RowBox[{"10", "^", 
         RowBox[{"-", "4"}]}]}], ",", "\"\<E6\>\"", ",", 
       RowBox[{
        RowBox[{
         RowBox[{"Sin", "[", "theta", "]"}], "^", "4"}], "+", 
        RowBox[{"10", "^", 
         RowBox[{"-", "4"}]}]}], ",", "\"\<E7\>\"", ",", 
       RowBox[{
        RowBox[{
         RowBox[{"Sin", "[", "theta", "]"}], "^", "5"}], "+", 
        RowBox[{"10", "^", 
         RowBox[{"-", "4"}]}]}], ",", "\"\<E8\>\"", ",", 
       RowBox[{
        RowBox[{
         RowBox[{"Sin", "[", "theta", "]"}], "^", "6"}], "+", 
        RowBox[{"10", "^", 
         RowBox[{"-", "4"}]}]}], ",", "_", ",", 
       RowBox[{
        RowBox[{
         RowBox[{"Sin", "[", "theta", "]"}], "*", 
         RowBox[{
          RowBox[{"Cos", "[", "phi", "]"}], "^", "2"}]}], "+", 
        RowBox[{"10", "^", 
         RowBox[{"-", "4"}]}]}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
   "\n", 
   RowBox[{"(*", 
    RowBox[{
    "Function", " ", "to", " ", "Calculate", " ", "Symmetry", " ", "Orbit", 
     " ", "Entropy", " ", 
     RowBox[{"(", "SOE", ")"}], " ", "for", " ", "Each", " ", "Group"}], 
    "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"calculateSOE", "[", "group_", "]"}], ":=", 
     RowBox[{"Module", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"densityIntegral", ",", "logIntegral", ",", "soe"}], "}"}], 
       ",", 
       RowBox[{
        RowBox[{"Print", "[", 
         RowBox[{
         "\"\<Calculating SOE for \>\"", ",", "group", ",", "\"\<...\>\""}], 
         "]"}], ";", "\[IndentingNewLine]", 
        RowBox[{"densityIntegral", "=", 
         RowBox[{"NIntegrate", "[", 
          RowBox[{
           RowBox[{"densityFunction", "[", 
            RowBox[{"group", ",", "theta", ",", "phi"}], "]"}], ",", 
           RowBox[{"{", 
            RowBox[{"theta", ",", "0", ",", "Pi"}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"phi", ",", "0", ",", 
             RowBox[{"2", " ", "Pi"}]}], "}"}], ",", 
           RowBox[{"Method", "->", "\"\<GlobalAdaptive\>\""}], ",", 
           RowBox[{"MaxRecursion", "->", "20"}], ",", 
           RowBox[{"PrecisionGoal", "->", "8"}]}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"logIntegral", "=", 
         RowBox[{"NIntegrate", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"Max", "[", 
             RowBox[{
              RowBox[{"densityFunction", "[", 
               RowBox[{"group", ",", "theta", ",", "phi"}], "]"}], ",", 
              RowBox[{"10", "^", 
               RowBox[{"-", "4"}]}]}], "]"}], "*", 
            RowBox[{"Log", "[", 
             RowBox[{"Max", "[", 
              RowBox[{
               RowBox[{"densityFunction", "[", 
                RowBox[{"group", ",", "theta", ",", "phi"}], "]"}], ",", 
               RowBox[{"10", "^", 
                RowBox[{"-", "4"}]}]}], "]"}], "]"}]}], ",", 
           RowBox[{"{", 
            RowBox[{"theta", ",", "0", ",", "Pi"}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"phi", ",", "0", ",", 
             RowBox[{"2", " ", "Pi"}]}], "}"}], ",", 
           RowBox[{"Method", "->", "\"\<GlobalAdaptive\>\""}], ",", 
           RowBox[{"MaxRecursion", "->", "20"}], ",", 
           RowBox[{"PrecisionGoal", "->", "8"}]}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"soe", "=", 
         RowBox[{
          RowBox[{"-", 
           RowBox[{"haarMeasure", "[", "group", "]"}]}], "*", 
          "logIntegral"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"Print", "[", 
         RowBox[{
         "\"\<Density Integral for \>\"", ",", "group", ",", "\"\<: \>\"", 
          ",", "densityIntegral"}], "]"}], ";", "\[IndentingNewLine]", 
        RowBox[{"Print", "[", 
         RowBox[{
         "\"\<Log Integral for \>\"", ",", "group", ",", "\"\<: \>\"", ",", 
          "logIntegral"}], "]"}], ";", "\[IndentingNewLine]", 
        RowBox[{"Print", "[", 
         RowBox[{
         "\"\<Entropy (SOE) for \>\"", ",", "group", ",", "\"\<: \>\"", ",", 
          "soe"}], "]"}], ";", "\[IndentingNewLine]", "soe"}]}], "]"}]}], 
    ";"}], "\[IndentingNewLine]", "\n", 
   RowBox[{"(*", 
    RowBox[{
    "List", " ", "of", " ", "Groups", " ", "to", " ", "Test", " ", "with", 
     " ", "Specified", " ", "Density", " ", "Functions", " ", "and", " ", 
     "Haar", " ", "Measures"}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
